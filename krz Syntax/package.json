{
	"name": "kerzenlicht-language",
	"displayName": "Kerzenlicht Language",
	"description": "Syntax Highlighting for .krz files in VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.91.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [{
			"id": "krz",
			"aliases": ["kerz", "krz"],
			"extensions": [".krz"],
			"configuration": "./language-configuration.json"
		}],
			"grammars": [{
			"language": "krz",
			"scopeName": "main.krz",
			"path": "./syntaxes/krz.tmLanguage.json"
		}],
		"themes": [{
			"label": "Your Language Theme",
			"uiTheme": "vs-dark",
			"path": "./themes/theme.json"
		}]
	}
}
